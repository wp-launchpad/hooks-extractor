{
  "name": "crochetfeve0251/rocket-launcher-hooks-extractor",
  "description": "Uninstaller library for psr4 plugin",
  "minimum-stability": "stable",
  "license": "proprietary",
  "type": "library",
  "authors": [
    {
      "name": "coquardcyr",
      "email": "coquardcyr@gmail.com"
    }
  ],
  "require": {
    "crochetfeve0251/rocket-launcher-builder": "^1.0.0",
    "ext-mbstring": "*",
    "ext-json": "*",
    "composer/composer": "^2.5",
    "jasny/phpdoc-parser": "^1.0",
    "ext-yaml": "*",
    "microsoft/tolerant-php-parser": "^0.1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8 || ^9",
    "wp-media/phpunit": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "RocketLauncherHooksExtractor\\": "inc/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RocketLauncherHooksExtractor\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
